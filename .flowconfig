[ignore]
.*/node_modules/.*
.*/dist/.*
.*/src/stories

[include]
./src/**/*.js

[libs]
flow-typed

[lints]

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.mjs
module.file_ext=.json
module.file_ext=.sass
module.file_ext=.scss
module.name_mapper='.*\(.s?css\)' -> 'empty/object'

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import